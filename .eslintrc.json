{
    "env": {
        "jest": true
    },
    "extends": ["eslint:recommended", "airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error",
            {
                "singleQuote": true, 
                "trailingComma": "all"
            }
        ],
        "no-console": ["error", { "allow": ["warn", "error"] }],
        "eqeqeq": ["error", "always"]
    }
}